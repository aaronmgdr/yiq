{
  "name": "yiq",
  "version": "2.0.3",
  "description": "Returns white when a color is dark and black when a color is light.",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "yarn check-all && del ./dist && tsc",
    "format": "prettier --config .prettierrc.json --write 'src/**/*'",
    "lint": "tslint --project tsconfig.json",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "coverage:watch": "jest --coverage --watch",
    "check-all": "yarn format && yarn lint && yarn test",
    "precommit": "yarn lint-staged && yarn lint && yarn test",
    "commit": "git-cz",
    "prepublish": "yarn build"
  },
  "husky": {
    "hooks": {
      "precommit": "yarn precommit"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,md}": [
      "yarn format",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/jest": "^23.3.11",
    "del-cli": "^1.1.0",
    "git-cz": "^1.8.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lffg/yiq.git"
  },
  "keywords": [
    "yiq",
    "color",
    "black",
    "white",
    "light",
    "dark",
    "color-convert"
  ],
  "author": "Luiz Felipe F. <https://lffg.github.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lffg/yiq/issues"
  },
  "homepage": "https://github.com/lffg/yiq#readme"
}
